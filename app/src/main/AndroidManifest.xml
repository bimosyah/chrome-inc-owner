<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ninda.sabilla.chromeincowner">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="ninda.sabilla.chromeincowner.utils.Notification"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MyTheme2"
        android:usesCleartextTraffic="true">
        <activity android:name="ninda.sabilla.chromeincowner.ui.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ninda.sabilla.chromeincowner.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/MyTheme2">
        </activity>
    </application>

</manifest>